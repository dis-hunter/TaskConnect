{
  "expo": {
    "name": "myServiceApp",
    "slug": "myServiceApp",
    "version": "1.0.0",
    "scheme": "myserviceapp",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "android": {
      "package": "com.taskconnect.myserviceapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "myserviceapp"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.taskconnect.myserviceapp"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-eas-project-id"
      }
    }
  }
}